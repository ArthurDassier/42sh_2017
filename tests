[304]
NAME="variables - utilisation avanc√©e (option)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin:.'"
CLEAN=""
TESTS=
  echo "set a=b"
  echo 'echo plop$a plip'
  echo 'echo bon${a}soir'
  echo 'ls $HOME'
  echo 'set b = pwd'
  echo '$b'
[304-END]

[305]
