[102-END]

[103]
NAME="Pipe with builtin"
SETUP="export PLOP=COUCOU ; OLDPATH=$PATH ; PATH='/toto'"
CLEAN="PATH=$OLDPATH"
TESTS=
  echo 'setenv | /bin/grep PLOP'
  echo '/bin/ls | cd rsc'
  echo 'ls -l'
  echo 'exit 45|cat /etc/resolv.conf'
[103-END]

