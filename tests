[001]
NAME="alias with arg (option)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin:.'"
CLEAN=""
TESTS=
  echo 'alias k file /tmp'
  echo 'k /bin'
[311-END]

[002]
NAME="alias of alias (option)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin:.'"
CLEAN=""
TESTS=
  echo 'alias k file /tmp'
  echo 'alias r k /bin'
  echo 'r .'
[312-END]

[003]
NAME="alias"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin:.'"
CLEAN=""
TESTS=
  echo 'alias lol mdr'
  echo 'ls mdr'
  echo 'alias'
[003-END]

[004]
NAME="alias"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin:.'"
CLEAN=""
TESTS=
  echo 'alias lol mdr'
  echo 'ls mdr'
  echo 'mdr lol'
  echo 'alias'
  echo 'alias lol mdr trop marrant'
[004-END]