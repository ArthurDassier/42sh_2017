[001]
NAME="Rien"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo ''
[001-END]

[002]
NAME="Simple exec"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'pwd'
  echo 'id'
[002-END]

